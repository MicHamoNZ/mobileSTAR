        {
          "name": "Delivery Job Batch",
          "event": [
            {
              "listen": "test",
              "script": {
                "id": "ad5f6ed6-ac40-452a-87c5-22db30417ac7",
                "exec": [
                  "var jsonData = pm.response.json();\r",
                  "var regexId = /^[0-9a-f]{8}-([0-9a-f]{4}-){3}[0-9a-f]{12}$/;\r",
                  "var regexBarCode = /^[a-z|A-Z]{3}[0-9]{10}$/;\r",
                  "\r",
                  "pm.test(\"Workorder Id is not null\", function () {\r",
                  "\tpm.expect(jsonData.Successful[0].Id).not.eql(null);\r",
                  "});\r",
                  "\r",
                  "pm.test(\"Workorder Id is not 0\", function () {\r",
                  "\tpm.expect(jsonData.Successful[0].Id).not.eql(0);\r",
                  "});\r",
                  "\r",
                  "pm.test(\"Workorder Id is a GUID\", function () {\r",
                  "\tpm.expect(regexId.test(jsonData.Successful[0].Id));\r",
                  "});\r",
                  "\r",
                  "pm.test(\"PackageId is not null\", function () {\r",
                  "\tpm.expect(jsonData.Successful[0].Children[0].Id).not.eql(null);\r",
                  "});\r",
                  "\r",
                  "pm.test(\"PackageId is not 0\", function () {\r",
                  "\tpm.expect(jsonData.Successful[0].Children[0].Id).not.eql(0);\r",
                  "});\r",
                  "\r",
                  "pm.test(\"PackageId is a GUID\", function () {\r",
                  "\tpm.expect(regexId.test(jsonData.Successful[0].Children[0].Id));\r",
                  "});\r",
                  "\r",
                  "pm.test(\"Package ExternalId is regex ^[a-z|A-Z]{3}[0-9]{10}$\", function () {\r",
                  "\tpm.expect(regexBarCode.test(jsonData.Successful[0].Children[0].ExternalId));\r",
                  "});\r",
                  "\r",
                  "pm.environment.set(\"packageId\", jsonData.Successful[0].Children[0].Id);\r",
                  "pm.environment.set(\"packageExternalId\", jsonData.Successful[0].Children[0].ExternalId);\r",
                  "\r",
                  "// console.log(\"WorkorderId:\", jsonData.Successful[0].Id);\r",
                  "// console.log(\"WorkorderExternalId:\", jsonData.Successful[0].ExternalId);\r",
                  "// console.log(\"PackageId\", jsonData.Successful[0].Children[0].Id);\r",
                  "// console.log(\"PackageExternalId\", jsonData.Successful[0].Children[0].ExternalId);"
                ],
                "type": "text/javascript"
              }
            },
            {
              "listen": "prerequest",
              "script": {
                "id": "ebbf98d7-e5af-4df7-b92c-4a8d43885667",
                "exec": [
                  "pm.variables.set('timestamp', pm.variables.replaceIn(\"{{$timestamp}}\"));\r",
                  "pm.variables.set(\"jobExternalId\",\"JOBDMH{{timestamp}}\");\r",
                  "pm.variables.set(\"packageBarcode\",\"SNR{{timestamp}}\");"
                ],
                "type": "text/javascript"
              }
            }
          ],
